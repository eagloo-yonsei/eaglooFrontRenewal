{
  "name": "eagloo-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "format": "lint-staged",
    "huskyinstall": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "format:all": "prettier --write \"src/**/*.{js,jsx,json,ts,tsx}\"",
    "start": "next start -p $PORT",
    "dev": "env-cmd -f ./env/.dev next dev -p 4001",
    "dev:proxy": "env-cmd -f ./env/.dev node proxy.server.js",
    "build:dev": "env-cmd -f ./env/.dev next build",
    "build:production": "env-cmd -f ./env/.prod next build && env-cmd -f ./env/.prod next export"
  },
  "dependencies": {
    "@alfonmga/react-lottie-light-ts": "0.0.1",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "0.24.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "dotenv": "10.0.0",
    "env-cmd": "10.1.0",
    "moment": "2.29.1",
    "next": "12.0.7",
    "next-compose-plugins": "2.2.1",
    "next-transpile-modules": "9.0.0",
    "nookies": "2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.20.4",
    "react-query": "3.33.4",
    "react-reset": "0.0.0",
    "react-toastify": "^7.0.4",
    "sha3": "^2.1.4",
    "simple-peer": "^9.11.0",
    "socket.io-client": "^4.1.3",
    "styled-components": "5.3.3",
    "styled-reset": "4.3.4",
    "swiper": "7.3.2",
    "typescript": "4.5.2",
    "urlencoded-body-parser": "3.0.0",
    "zustand": "3.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/react": "17.0.36",
    "@types/react-dom": "17.0.11",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-styled-components": "2.0.2",
    "cors": "2.8.5",
    "cross-env": "^7.0.3",
    "eslint-plugin-import": "2.25.3",
    "express": "4.17.1",
    "http-proxy-middleware": "2.0.1",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "parcel-bundler": "^1.12.5",
    "prettier": "2.5.1",
    "react-hot-loader": "^4.13.0",
    "simple-zustand-devtools": "1.0.1"
  }
}
